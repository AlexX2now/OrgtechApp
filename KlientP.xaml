<Window x:Class="UP_2.KlientP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP_2"
        mc:Ignorable="d"
        Title="Страница клиента" Height="450" Width="318">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Клиент" VerticalAlignment="Top" FontSize="20"/>
        <Button x:Name="exit" Content="Выйти из аккаунта" HorizontalAlignment="Center" Margin="0,375,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Click="exit_Click"/>
        <Button x:Name="createzayav" Content="Создать заявку" HorizontalAlignment="Center" Margin="0,68,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Width="231" Click="createzayav_Click"/>
        <Button x:Name="redzayav" Content="Редактировать заявку" HorizontalAlignment="Center" Margin="0,104,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Width="231" Click="redzayav_Click"/>
        <Button x:Name="toz" Content="Просмотр своих заявок" HorizontalAlignment="Center" Margin="0,140,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Width="231" Click="toz_Click"/>

    </Grid>
</Window>
