<Window x:Class="UP_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP_2"
        mc:Ignorable="d"
        Title="Глвная страница" Height="450" Width="361">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Авторизация" VerticalAlignment="Top" FontSize="22"/>
        <TextBox x:Name="login" HorizontalAlignment="Center" Margin="0,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="218" FontSize="18" Text="login4"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,71,0,0" TextWrapping="Wrap" Text="Логин" VerticalAlignment="Top" FontSize="18"/>
        <TextBox x:Name="password" HorizontalAlignment="Center" Margin="0,234,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="218" FontSize="18" Text="pass4"/>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Пароль" VerticalAlignment="Center" FontSize="18"/>
        <Button x:Name="authbtn" Content="Войти" HorizontalAlignment="Center" Margin="0,347,0,0" VerticalAlignment="Top" Background="White" FontSize="22" Width="174" Click="authbtn_Click"/>

    </Grid>
</Window>
