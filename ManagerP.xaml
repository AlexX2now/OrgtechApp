<Window x:Class="UP_2.ManagerP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP_2"
        mc:Ignorable="d"
        Title="Страница менеджера" Height="450" Width="318">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Менеджер" VerticalAlignment="Top" FontSize="20"/>
        <Button x:Name="exit" Content="Выйти из аккаунта" HorizontalAlignment="Center" Margin="0,375,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Click="exit_Click"/>
        <Button Content="Назначить мастера" HorizontalAlignment="Center" Margin="0,68,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Width="231"/>
        <Button Content="Продлить срок заявки" HorizontalAlignment="Center" Margin="0,104,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Width="231"/>
        <Image x:Name="Imagee" HorizontalAlignment="Left" Height="199" Margin="21,157,0,0" VerticalAlignment="Top" Width="230"/>

    </Grid>
</Window>
